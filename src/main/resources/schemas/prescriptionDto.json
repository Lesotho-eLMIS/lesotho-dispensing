{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "Prescription",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "patientId": {
      "type": "string",
      "format": "uuid"
    },
    "patientType": {
      "type": "string"
    },
    "followUpDate": {
      "type": "string",
      "format": "date"
    },
    "issueDate": {
      "type": "string",
      "format": "date"
    },
    "createdDate": {
      "type": "string",
      "format": "date"
    },
    "capturedDate": {
      "type": "string",
      "format": "date"
    },
    "lastUpdate": {
      "type": "string",
      "format": "date"
    },
    "isVoided": {
      "type": "boolean"
    },
    "status": {
      "type": "string"
    },
    "facilityId": {
      "type": "string",
      "format": "uuid"
    },
    "userId": {
      "type": "string",
      "format": "uuid"
    },
    "lineItems": {
      "type": "array",
      "items": {
        "$ref": "prescriptionLineItemDto.json"
      }
    }
  },
  "required": ["id", "patientId", "issueDate", "createdDate", "facilityId", "userId"]
}