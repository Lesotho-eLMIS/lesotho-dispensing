{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "Patient",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "facilityId": {
      "type": "string",
      "format": "uuid"
    },
    "person": {
      "$ref": "personDto.json"
    },
    "medicalHistory": {
      "type": "array",
      "items": {
        "$ref": "medicalHistoryDto.json"
      }
    }
  },
  "required": ["facilityId", "person"]
}